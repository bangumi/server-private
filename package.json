{
  "name": "app",
  "version": "0.28.0",
  "description": "",
  "private": true,
  "type": "module",
  "main": "bin/main.ts",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": "^22.11"
  },
  "scripts": {
    "drizzle-pull": "drizzle-kit introspect",
    "format": "prettier --list-different -w ./",
    "tsc": "tsc",
    "lint": "eslint -c eslint.config.mjs ./",
    "build": "node build.mjs",
    "build-common": "node scripts/build-common.mjs",
    "start": "nodemon ./bin/main.ts",
    "start-cron": "nodemon ./bin/cron.ts",
    "start-mq": "nodemon ./bin/mq.ts",
    "test": "vitest --run",
    "vitest": "vitest",
    "file": "node --no-warnings --import tsx/esm",
    "e": "node --no-warnings -r dotenv/config --import tsx/esm",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.842.0",
    "@bgm38/wiki": "0.3.3",
    "@confluentinc/kafka-javascript": "1.3.2",
    "@fastify/cookie": "11.0.2",
    "@fastify/error": "4.2.0",
    "@fastify/formbody": "8.0.2",
    "@fastify/request-context": "6.2.0",
    "@fastify/static": "8.2.0",
    "@fastify/swagger": "9.5.1",
    "@fastify/type-provider-typebox": "5.1.0",
    "@fastify/view": "11.1.0",
    "@node-rs/bcrypt": "1.10.7",
    "@sentry/node": "9.34.0",
    "@sinclair/typebox": "0.34.37",
    "@trim21/php-serialize": "0.2.2",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "ajv-keywords": "5.1.0",
    "altair-static": "8.2.5",
    "cron": "4.3.1",
    "diff": "7.0.0",
    "drizzle-orm": "0.44.2",
    "fastify": "5.4.0",
    "fastify-metrics": "12.1.0",
    "fastify-socket.io": "5.1.0",
    "got": "14.4.7",
    "graphql": "16.11.0",
    "http-proxy-agent": "7.0.2",
    "http-status-codes": "2.3.0",
    "https-proxy-agent": "7.0.6",
    "ioredis": "5.6.1",
    "js-yaml": "4.1.0",
    "keyv": "5.3.4",
    "liquidjs": "10.21.1",
    "lodash-es": "4.17.21",
    "luxon": "3.6.1",
    "meilisearch": "0.51.0",
    "mercurius": "16.2.0",
    "micromatch": "4.0.8",
    "mime-types": "3.0.1",
    "mysql2": "3.14.1",
    "openapi-types": "12.1.3",
    "parse-duration": "2.1.4",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "prom-client": "15.1.3",
    "reflect-metadata": "0.2.2",
    "socket.io": "4.8.1",
    "socks-proxy-agent": "8.0.5",
    "typeorm": "0.3.25",
    "yaml": "2.8.0"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-resolvers": "4.5.1",
    "@shopify/prettier-plugin-liquid": "1.9.3",
    "@types/diff": "7.0.2",
    "@types/ioredis-mock": "8.2.6",
    "@types/js-yaml": "4.0.9",
    "@types/lodash-es": "4.17.12",
    "@types/luxon": "3.6.2",
    "@types/mime-types": "2.1.4",
    "@types/node": "22.15.30",
    "@typescript-eslint/eslint-plugin": "8.33.1",
    "@typescript-eslint/parser": "8.33.1",
    "@typescript-eslint/utils": "8.33.1",
    "@vitest/coverage-v8": "3.2.2",
    "cheerio": "1.0.0",
    "dotenv": "17.0.1",
    "drizzle-kit": "0.31.1",
    "esbuild": "0.25.5",
    "esbuild-node-externals": "1.18.0",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-erasable-syntax-only": "0.3.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "graphql-tag": "2.12.6",
    "husky": "9.1.7",
    "ioredis-mock": "8.9.0",
    "lint-staged": "16.1.0",
    "mercurius-integration-testing": "9.0.1",
    "nodemon": "3.1.10",
    "p-limit": "6.2.0",
    "prettier": "3.5.3",
    "prettier-plugin-jsdoc": "1.3.2",
    "std-env": "3.9.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1",
    "vite": "6.3.5",
    "vitest": "3.2.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**.test.ts"
    ],
    "watch": [
      "bin",
      "lib",
      "routes",
      "config.yaml",
      "package.json",
      "tsconfig.json",
      ".env"
    ],
    "delay": 100,
    "execMap": {
      "ts": "node --no-warnings -r dotenv/config --import tsx/esm"
    },
    "env": {
      "NODE_ENV": "development",
      "REF": "development"
    },
    "ext": "ts,js,json,html"
  },
  "author": "",
  "prettier": {
    "plugins": [
      "@shopify/prettier-plugin-liquid",
      "prettier-plugin-jsdoc"
    ],
    "tsdoc": true,
    "printWidth": 100,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@confluentinc/kafka-javascript",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "fastify-socket.io"
    ],
    "patchedDependencies": {
      "fastify-socket.io": "patches/fastify-socket.io.patch"
    }
  }
}
