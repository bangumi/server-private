    "prom-client": "^14.1.1",
    "proxy-agent": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.6.0",
    "typeorm": "^0.3.12"
  },
  "prettier": {
    "plugins": [
      "@shopify/prettier-plugin-liquid",
      "prettier-plugin-jsdoc"
    ],
    "tsdoc": true,
    "printWidth": 100,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  }
}
