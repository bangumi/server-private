{
  "name": "app",
  "version": "0.6.0",
  "description": "",
  "private": true,
  "type": "module",
  "main": "bin/main.ts",
  "packageManager": "pnpm@8.11.0",
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  },
  "scripts": {
    "format": "prettier --list-different -w ./",
    "tsc": "tsc",
    "lint": "eslint --ext ts,cjs,js ./",
    "start": "nodemon ./bin/main.ts",
    "test": "vitest --run",
    "vitest": "vitest",
    "file": "node --no-warnings --loader @esbuild-kit/esm-loader",
    "license-check": "license-checker --excludePrivatePackages --production --summary --onlyAllow=\"0BSD;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;MIT;Python-2.0;\"",
    "e": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader",
    "lint-staged": "lint-staged -q",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.462.0",
    "@bgm38/wiki": "^0.2.6",
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@fastify/cookie": "^9.2.0",
    "@fastify/error": "^3.4.1",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/view": "^8.2.0",
    "@node-rs/bcrypt": "^1.7.3",
    "@sinclair/typebox": "~0.31.28",
    "@trim21/php-serialize": "^0.1.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "altair-fastify-plugin": "^5.2.11",
    "fastify": "^4.24.3",
    "fastify-metrics": "^10.4.1",
    "fastify-socket.io": "^5.0.0",
    "got": "^13.0.0",
    "graphql": "^16.8.1",
    "http-proxy-agent": "^7.0.0",
    "http-status-codes": "^2.3.0",
    "https-proxy-agent": "^7.0.2",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.9.4",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.4",
    "mercurius": "^13.3.1",
    "micromatch": "^4.0.5",
    "mime-types": "^2.1.35",
    "mysql2": "^3.6.5",
    "nexus": "^1.3.0",
    "node-cache": "^5.1.2",
    "openapi-types": "^12.1.3",
    "parse-duration": "^1.1.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "socks-proxy-agent": "^8.0.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "^1.3.4",
    "@types/ioredis-mock": "^8.2.5",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.3.6",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@typescript-eslint/utils": "^6.13.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "ioredis-mock": "^8.9.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.1.0",
    "mercurius-integration-testing": "^8.1.1",
    "nodemon": "^3.0.1",
    "p-limit": "^5.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-jsdoc": "^1.1.1",
    "std-env": "^3.5.0",
    "typescript": "~5.3.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vitest-github-actions-reporter": "^0.11.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**.test.ts"
    ],
    "watch": [
      "bin",
      "lib",
      "routes",
      "config.yaml",
      "package.json",
      "tsconfig.json",
      ".env"
    ],
    "delay": 100,
    "execMap": {
      "ts": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader"
    },
    "env": {
      "NODE_ENV": "development",
      "REF": "development"
    },
    "ext": "ts,js,json,html"
  },
  "author": "",
  "prettier": {
    "plugins": [
      "@shopify/prettier-plugin-liquid",
      "prettier-plugin-jsdoc"
    ],
    "tsdoc": true,
    "printWidth": 100,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "pnpm": {
    "patchedDependencies": {}
  }
}
