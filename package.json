{
  "name": "app",
  "version": "0.21.3",
  "description": "",
  "private": true,
  "type": "module",
  "main": "bin/main.ts",
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": "^22.11"
  },
  "scripts": {
    "drizzle-pull": "drizzle-kit introspect",
    "format": "prettier --list-different -w ./",
    "tsc": "tsc",
    "lint": "eslint -c eslint.config.mjs ./",
    "build": "node build.mjs",
    "build-common": "node scripts/build-common.mjs",
    "start": "nodemon ./bin/main.ts",
    "start-cron": "nodemon ./bin/cron.ts",
    "start-mq": "nodemon ./bin/mq.ts",
    "test": "vitest --run",
    "vitest": "vitest",
    "file": "node --no-warnings --import tsx/esm",
    "e": "node --no-warnings -r dotenv/config --import tsx/esm",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.758.0",
    "@bgm38/wiki": "0.3.3",
    "@confluentinc/kafka-javascript": "1.2.0",
    "@fastify/cookie": "11.0.2",
    "@fastify/error": "4.0.0",
    "@fastify/formbody": "8.0.2",
    "@fastify/request-context": "6.1.0",
    "@fastify/static": "8.1.1",
    "@fastify/swagger": "9.4.2",
    "@fastify/type-provider-typebox": "5.1.0",
    "@fastify/view": "10.0.2",
    "@node-rs/bcrypt": "1.10.7",
    "@sentry/node": "9.5.0",
    "@sinclair/typebox": "0.34.28",
    "@trim21/php-serialize": "0.1.1",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "ajv-keywords": "5.1.0",
    "altair-static": "8.2.0",
    "cron": "4.1.0",
    "drizzle-orm": "0.40.0",
    "fastify": "5.2.1",
    "fastify-metrics": "12.1.0",
    "fastify-socket.io": "5.1.0",
    "got": "14.4.6",
    "graphql": "16.10.0",
    "http-proxy-agent": "7.0.2",
    "http-status-codes": "2.3.0",
    "https-proxy-agent": "7.0.6",
    "ioredis": "5.6.0",
    "js-yaml": "4.1.0",
    "liquidjs": "10.21.0",
    "lodash-es": "4.17.21",
    "luxon": "3.5.0",
    "meilisearch": "0.49.0",
    "mercurius": "16.1.0",
    "micromatch": "4.0.8",
    "mime-types": "2.1.35",
    "mysql2": "3.13.0",
    "node-cache": "5.1.2",
    "openapi-types": "12.1.3",
    "parse-duration": "2.1.3",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "prom-client": "15.1.3",
    "reflect-metadata": "0.2.2",
    "socket.io": "4.8.1",
    "socks-proxy-agent": "8.0.5",
    "typeorm": "0.3.21",
    "yaml": "2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.5",
    "@graphql-codegen/typescript-resolvers": "4.4.4",
    "@shopify/prettier-plugin-liquid": "1.9.0",
    "@types/ioredis-mock": "8.2.5",
    "@types/js-yaml": "4.0.9",
    "@types/lodash-es": "4.17.12",
    "@types/luxon": "3.4.2",
    "@types/mime-types": "2.1.4",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "@typescript-eslint/utils": "8.26.0",
    "@vitest/coverage-v8": "3.0.8",
    "cheerio": "1.0.0",
    "dotenv": "16.4.7",
    "drizzle-kit": "0.30.5",
    "esbuild": "0.25.0",
    "esbuild-node-externals": "1.18.0",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tsdoc": "0.4.0",
    "eslint-plugin-unicorn": "57.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "graphql-tag": "2.12.6",
    "husky": "9.1.7",
    "ioredis-mock": "8.9.0",
    "lint-staged": "15.4.3",
    "mercurius-integration-testing": "9.0.1",
    "nodemon": "3.1.9",
    "p-limit": "6.2.0",
    "prettier": "3.5.3",
    "prettier-plugin-jsdoc": "1.3.2",
    "std-env": "3.8.1",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0",
    "vite": "6.2.1",
    "vitest": "3.0.8"
  },
  "nodemonConfig": {
    "ignore": [
      "**.test.ts"
    ],
    "watch": [
      "bin",
      "lib",
      "routes",
      "config.yaml",
      "package.json",
      "tsconfig.json",
      ".env"
    ],
    "delay": 100,
    "execMap": {
      "ts": "node --no-warnings -r dotenv/config --import tsx/esm"
    },
    "env": {
      "NODE_ENV": "development",
      "REF": "development"
    },
    "ext": "ts,js,json,html"
  },
  "author": "",
  "prettier": {
    "plugins": [
      "@shopify/prettier-plugin-liquid",
      "prettier-plugin-jsdoc"
    ],
    "tsdoc": true,
    "printWidth": 100,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@confluentinc/kafka-javascript",
      "esbuild"
    ],
    "patchedDependencies": {
      "fastify-socket.io": "patches/fastify-socket.io.patch"
    }
  }
}
