{
  "name": "graphql",
  "version": "0.2.1",
  "description": "",
  "private": true,
  "type": "module",
  "main": "lib/main.ts",
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "format": "prettier -w ./",
    "lint": "eslint --cache --ext ts,cjs,js ./",
    "start": "nodemon ./lib/main.ts",
    "watch-less": "less-watch-compiler --enable-js style static/style",
    "watch-livereload": "livereload 'templates, static' --extraExts liquid",
    "test": "vitest --run",
    "vitest": "vitest",
    "run-file": "node --no-warnings --loader @esbuild-kit/esm-loader",
    "e": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader",
    "prepare": "husky install"
  },
  "nodemonConfig": {
    "ignore": [
      "**.test.ts"
    ],
    "watch": [
      "lib",
      "routes",
      "config.yaml",
      "package.json",
      "tsconfig.json",
      ".env"
    ],
    "delay": 100,
    "execMap": {
      "ts": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader"
    },
    "env": {
      "NODE_ENV": "development",
      "REF": "development"
    },
    "ext": "ts,js,json,html"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "^1.0.6",
    "@types/ioredis-mock": "^8.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^3.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/minio": "^7.0.15",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@typescript-eslint/utils": "^5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/ui": "^0.28.5",
    "conventional-changelog": "^3.1.25",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "ioredis-mock": "^8.2.6",
    "less": "^4.1.3",
    "less-watch-compiler": "^1.16.3",
    "lint-staged": "^13.1.2",
    "livereload": "^0.9.3",
    "mercurius-integration-testing": "^6.0.1",
    "nodemon": "^2.0.20",
    "p-limit": "^4.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-sh": "^0.12.8",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.5",
    "vitest-github-actions-reporter": "^0.9.0"
  },
  "dependencies": {
    "@bgm38/wiki": "^0.2.2",
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@fastify/cookie": "^8.3.0",
    "@fastify/error": "^3.2.0",
    "@fastify/static": "^6.9.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@fastify/view": "^7.4.1",
    "@node-rs/bcrypt": "^1.6.2",
    "@sinclair/typebox": "^0.25.21",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "altair-fastify-plugin": "^5.0.15",
    "fastify": "^4.13.0",
    "fastify-metrics": "^10.0.3",
    "fastify-socket.io": "^4.0.0",
    "got": "^12.5.3",
    "graphql": "^16.6.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.1",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.4.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.2.1",
    "mercurius": "^12.0.1",
    "mime-types": "^2.1.35",
    "minio": "^7.0.32",
    "mysql2": "^3.1.2",
    "nanoid": "^4.0.1",
    "nexus": "^1.3.0",
