{
  "name": "app",
  "version": "0.5.13",
  "description": "",
  "private": true,
  "type": "module",
  "main": "bin/main.ts",
  "packageManager": "pnpm@8.7.5",
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  },
  "scripts": {
    "format": "prettier --list-different -w ./",
    "tsc": "tsc",
    "lint": "eslint --ext ts,cjs,js ./",
    "start": "nodemon ./bin/main.ts",
    "test": "vitest --run",
    "vitest": "vitest",
    "file": "node --no-warnings --loader @esbuild-kit/esm-loader",
    "license-check": "license-checker --excludePrivatePackages --production --summary --onlyAllow=\"0BSD;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;MIT;Python-2.0;\"",
    "e": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader",
    "lint-staged": "lint-staged -q",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@bgm38/wiki": "^0.2.6",
    "@esbuild-kit/esm-loader": "^2.6.4",
    "@fastify/cookie": "^9.0.4",
    "@fastify/error": "^3.3.0",
    "@fastify/static": "^6.10.2",
    "@fastify/swagger": "^8.9.0",
    "@fastify/view": "^8.0.0",
    "@node-rs/bcrypt": "^1.7.3",
    "@sinclair/typebox": "~0.31.8",
    "@trim21/php-serialize": "^0.1.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "altair-fastify-plugin": "^5.2.2",
    "fastify": "^4.22.2",
    "fastify-metrics": "^10.3.0",
    "fastify-socket.io": "^4.0.0",
    "got": "^13.0.0",
    "graphql": "^16.8.0",
    "http-proxy-agent": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "https-proxy-agent": "^7.0.1",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.9.2",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.2",
    "mercurius": "^13.1.0",
    "micromatch": "^4.0.5",
    "mime-types": "^2.1.35",
    "mysql2": "^3.6.0",
    "nexus": "^1.3.0",
    "node-cache": "^5.1.2",
    "openapi-types": "^12.1.3",
    "parse-duration": "^1.1.0",
